<p>Books</p>
<% @books.each do |book| %>
  <p>title</p>
  <span><%= book.title %></span>
  <p>body</p>
  <span><%= book.body %></span>
<% end %>

<h1>New book</h1>

<%= form_with model: @book, url: '/books', method: :post do |f| %>
  <h4>title</h4>
  <%= f.text_field :title %>
	
  <h4>body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
  
  
<% end %>
